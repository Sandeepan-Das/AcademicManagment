<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="UTF-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>Marks Page</title>
</head>
<body id="body">
    

        <div id="wrapper">
            <table align='center' cellspacing=1 cellpadding=1 id="mytable" border=1>
                <thead>
                    <tr>
                        <th>Id</th>
                        <th>Name</th>
                        <th>Quiz</th>
                        <th>TA</th>
                        <th>Midsem</th>
                        <th>Endsem</th>
                        <th>Action</th>
                    </tr>
                </thead>
                <tbody id="tbody">
                    <% students.forEach(function(student){ %>
                        <tr>
                            <td><%= student.roll %></td>
                            <td><%= student.name %></td>
                            <td id="quiz_row<%= student.no %>"><%= student.quiz %></td>
                            <td id="ta_row<%= student.no %>"><%= student.ta %></td>
                            <td id="midsem_row<%= student.no %>"><%= student.midsem %></td>
                            <td id="endsem_row<%= student.no %>"><%= student.endsem %></td>
                            <td><input type="button" id="edit_button<%= student.no %>" value="Edit" class="edit" onclick="edit_row('<%= student.no %>')">
                                <input type="button" id="save_button<%= student.no %>" value="Save" class="save" onclick="save_row('<%= student.no %>')"></td>
                        </tr>
                        <% }); %>
                </tbody>
            </table>
        </div>
        
        <script src="../scripts/marks.js"></script>
    
</body>
</html>