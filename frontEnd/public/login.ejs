<!DOCTYPE html>
<html lang="en">
  <head>
    <meta charset="UTF-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1" />
    <!-- CSS only -->
    <link
      href="https://cdn.jsdelivr.net/npm/bootstrap@5.0.0-beta2/dist/css/bootstrap.min.css"
      rel="stylesheet"
      integrity="sha384-BmbxuPwQa2lc/FVzBcNJ7UAyJxM6wuqIj61tLrc4wSX0szH/Ev+nYRRuWlolflfl"
      crossorigin="anonymous"
    />

    <link
      rel="stylesheet"
      type="text/css"
      href="https://maxcdn.bootstrapcdn.com/font-awesome/4.4.0/css/font-awesome.min.css"
    />
    <title>A Great Demo on CodePen</title>
    <link rel="stylesheet" href="../stylesheets/login.css" />
  </head>
  <body>
    <%- include('../partials/header.ejs') %>
    <section class="login-block">
      <div class="container">
        <div class="row">
          <div class="col-md-4 login-sec">
            <h2 class="text-center">Login Now</h2>
            <form class="login-form" id="form">
              <label for="exampleInputEmail1" class="text-uppercase"
                >User Email ID</label
              >
              <div class="form-control">
                <input
                  type="text"
                  class="form-control"
                  placeholder=""
                  id="uid"
                />
                <i class="fa fa-check-circle"></i>
                <i class="fa fa-exclamation-cricle"></i>

                <small>error message</small>
              </div>
              <label for="exampleInputPassword1" class="text-uppercase"
                >Password</label
              >
              <div class="form-control">
                <input
                  type="password"
                  class="form-control"
                  placeholder=""
                  id="password"
                />
                <i class="fa fa-check-circle"></i>
                <i class="fa fa-exclamation-cricle"></i>

                <small>error message</small>
              </div>
              <button type="submit" class="btn btn-login">Submit</button>
            </form>
          </div>
          <div class="col-md-8 banner-sec"></div>
        </div>
      </div>
    </section>
    <footer><p>copyright..</p></footer>
    <script src="../scripts/login.js"></script>    
    <script src="../../backEnd/staticdB/subj.js"></script> 
    <%- include('../partials/footer.ejs') %>
  </body>
</html>
